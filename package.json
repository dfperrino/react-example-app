{
  "name": "prueba-at",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/luxon": "2.0.7",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "cypress": "9.0.0",
    "ramda": "0.27.1",
    "react-scripts": "4.0.3",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@mui/icons-material": "5.1.0",
    "@mui/material": "5.1.0",
    "axios": "0.24.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "luxon": "2.1.1",
    "notistack": "2.0.3",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "5.21.0",
    "react-router-dom": "6.0.1",
    "web-vitals": "1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:e2e": "cypress open",
    "test:coverage": "react-scripts test --coverage --watchAll=false"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/context"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.{d.ts}",
      "!src/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && node check-i18n.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
